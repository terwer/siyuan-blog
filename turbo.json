{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "preview": {
      "dependsOn": ["^build"]
    },
    "doc": {
      "dependsOn": ["^build"]
    },
    "md": {
      "dependsOn": ["^doc"]
    },
    "api": {
      "dependsOn": ["^md"]
    }
  }
}
