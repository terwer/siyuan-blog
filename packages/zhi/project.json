{
    "name": "zhi",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/zhi/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "/Users/terwer/Documents/mydocs/SiYuanWorkspace/public/conf/appearance/themes/zhi",
            }
        },
        "publish": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node tools/scripts/publish.mjs zhi {args.ver} {args.tag}"
            },
            "dependsOn": ["build"]
        },
        "test": {
            "executor": "@nrwl/vite:test",
            "outputs": ["coverage/packages/zhi"],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/packages/zhi"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/zhi/**/*.ts"]
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "nx run zhi:build --watch"
            },
            "dependsOn": []
        }
    },
    "tags": []
}
