{
  "name": "zhi-blog-vuepress",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/zhi-blog-vuepress/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "export NODE_OPTIONS=--max_old_space_size=4096 && vuepress build packages/zhi-blog-vuepress/src"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "export NODE_OPTIONS=--max_old_space_size=4096 && vuepress dev packages/zhi-blog-vuepress/src"
      }
    }
  },
  "tags": []
}
