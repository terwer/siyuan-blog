{
  "name": "zhi-docs-vitepress",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/zhi-docs-vitepress/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node packages/zhi-docs-vitepress/src/makeMd.cjs && vitepress build packages/zhi-docs-vitepress"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress dev packages/zhi-docs-vitepress"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress preview packages/zhi-docs-vitepress"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
