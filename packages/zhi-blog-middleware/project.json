{
    "name": "zhi-blog-middleware",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/zhi-blog-middleware/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/zhi-blog-middleware/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/zhi-blog-middleware/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/zhi-blog-middleware",
                "tsConfig": "packages/zhi-blog-middleware/tsconfig.lib.json",
                "packageJson": "packages/zhi-blog-middleware/package.json",
                "main": "packages/zhi-blog-middleware/src/index.ts",
                "assets": ["packages/zhi-blog-middleware/*.md"]
            }
        }
    },
    "tags": []
}
