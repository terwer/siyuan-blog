{
    "name": "zhi-modules-blog-middleware-web",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/zhi-modules-blog-middleware-web/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                 "outputPath": "dist/packages/zhi/web-modules/blog-middleware",
            }
        },
        "publish": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node tools/scripts/publish.mjs zhi-modules-blog-middleware-web {args.ver} {args.tag}"
            },
            "dependsOn": ["build"]
        },
        "test": {
            "executor": "@nrwl/vite:test",
            "outputs": ["coverage/packages/zhi-modules-blog-middleware-web"],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/packages/zhi-modules-blog-middleware-web"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/zhi-modules-blog-middleware-web/**/*.ts"]
            }
        },
        "zhi-build": {
            "dependsOn": ["build"],
            "command": "cp -r dist/packages/zhi/web-modules/blog-middleware/* /Users/terwer/Documents/mydocs/SiYuanWorkspace/public/conf/appearance/themes/zhi/web-modules/blog-middleware && echo 'zhi-web-modules-blog-middleware dist copy success'"
        }
    },
    "tags": []
}
